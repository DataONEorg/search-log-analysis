      <procstep>
        <procdesc>Grid Input: Combine all the data into a single XYZ file using AWK.</procdesc>
        <procdate>2009</procdate>
        <proccont>
          <cntinfo>
            <cntperp>
              <cntper>Richard P. Signell</cntper>
