            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Particulate hydrocarbons for mesocosm incubations</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>Particulate hydrocarbons for mesocosm incubations in DISL, 2011</gco:CharacterString>
                    </gmd:alternateTitle>
