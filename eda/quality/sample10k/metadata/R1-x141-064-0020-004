            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Dataset for: Molecular Modeling of Green Leaf Volatile Methyl Salicylate on Atmospheric Air/Water Interfaces.</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
                    </gmd:alternateTitle>
                    <gmd:date>
                        <gmd:CI_Date>
