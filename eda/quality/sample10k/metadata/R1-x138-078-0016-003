            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Mesocosm, Dispersion of oil in beach sand with and without Corexit.</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>Corexit laboratory experiment</gco:CharacterString>
