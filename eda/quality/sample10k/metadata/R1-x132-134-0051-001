        <gmd:MD_DataIdentification>
            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>R/V Pelican CTD Cast Data, Gulf of Mexico, May 2010</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
                    </gmd:alternateTitle>
                    <gmd:date>
                        <gmd:CI_Date>
