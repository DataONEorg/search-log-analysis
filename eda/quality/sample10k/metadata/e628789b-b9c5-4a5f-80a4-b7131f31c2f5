        <lworkcit>
          <citeinfo>
            <origin>Julieta Gomez-Fragoso</origin>
            <origin>Heriberto Torres-Sierra</origin>
            <pubdate>2016</pubdate>
            <title>Dam Failure Analysis for the Lago El Guineo Dam, Orocovis, Puerto Rico</title>
            <geoform>Publication</geoform>
            <serinfo>
              <sername>Scientific Investigations Report</sername>
              <issue>2016-5070</issue>
            </serinfo>
            <pubinfo>
              <pubplace>Reston, VA, USA</pubplace>
              <publish>U.S. Geological Survey</publish>
            </pubinfo>
            <onlink>http://dx.doi.org/10.3133/sir20165070</onlink>
          </citeinfo>
        </lworkcit>
