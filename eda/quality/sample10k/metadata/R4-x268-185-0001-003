        <gmd:title>
          <gco:CharacterString>Water column chemistry above several seep sites, Gulf of Mexico, September 2012 - June 2013</gco:CharacterString>
        </gmd:title>
        <gmd:alternateTitle>
          <gco:CharacterString/>
        </gmd:alternateTitle>
