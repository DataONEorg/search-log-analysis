            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Zwitterionic Microgel for Adsorption and Release of Surfactant</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>ZI-MG for Adsorption and Release of Surfactant</gco:CharacterString>
