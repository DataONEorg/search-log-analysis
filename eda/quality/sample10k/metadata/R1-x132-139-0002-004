      <gmd:CI_Citation>
        <gmd:title>
          <gco:CharacterString>Profiling snowcam images near natural seeps, northern Gulf of Mexico September 2012</gco:CharacterString>
        </gmd:title>
        <gmd:alternateTitle>
          <gco:CharacterString/>
        </gmd:alternateTitle>
