    <citation>
      <citeinfo>
        <origin>T. Messinger</origin>
        <pubdate>20140327</pubdate>
        <title>Streamflow Correlation Map Grids in and near West Virginia 1930-2011</title>
        <geoform>Geodatabase layers and grids</geoform>
        <pubinfo>
          <pubplace>Reston, Virginia</pubplace>
