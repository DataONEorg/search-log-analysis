            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Heterocapsa abundance and its associated attached and free-living bacteria exposed to dispersant, dispersed and not dispersed crude oil</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
                    </gmd:alternateTitle>
                    <gmd:date>
