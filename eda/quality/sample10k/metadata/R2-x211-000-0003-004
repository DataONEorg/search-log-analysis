            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Photosynthetic pigment composition data (benthic microalgae) from Barataria Bay, 2011-2014</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>Benthic microalgae data</gco:CharacterString>
                    </gmd:alternateTitle>
                    <gmd:date>
