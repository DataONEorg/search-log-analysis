                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Vertical cast water column conductivity, temperature, and depth (CTD) and auxiliary data collected in the Gulf of Mexico from May through September 2010</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>Vertical Cast Water Column CTD Data; Study Cruises &#x2013; May through August 2010</gco:CharacterString>
                    </gmd:alternateTitle>
