            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Field data supporting study of temporal changes in microbial diversity from Louisiana marsh sediment and water, June 2012</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>Field Data Summary June 2012</gco:CharacterString>
