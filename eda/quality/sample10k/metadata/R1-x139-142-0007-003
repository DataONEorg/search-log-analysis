            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Observed and simulated hypoxic zone extent for the 2002 LaTex shelf wide cruise (July, 21-26)</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>FVCOM model grid and simulation results</gco:CharacterString>
