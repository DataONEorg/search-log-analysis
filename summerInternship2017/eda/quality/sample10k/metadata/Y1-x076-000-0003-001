            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Simulation of single component jet in a crossflow</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
                    </gmd:alternateTitle>
                    <gmd:date>
