            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Naphthalenes, Phenanthrenes, and Chrysenes in Northwest Florida Continental Shelf Sediments, May-June 2010</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
                    </gmd:alternateTitle>
                    <gmd:date>
                        <gmd:CI_Date>
