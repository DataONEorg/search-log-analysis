            <entityName>lngr_herpetofauna_survey_2015_exotic_species_data_p276996.csv</entityName>
            <entityDescription>Nanangroe herpetofauna search survey 2015 - sightings of exotic faunal species and non-arboreal marsupials</entityDescription>
            <physical id="1474340588806">
                <objectName>lngr_herpetofauna_survey_2015_exotic_species_data_p276996.csv</objectName>
                <size unit="byte">153</size>
                <dataFormat>
                    <textFormat>
                        <numHeaderLines>1</numHeaderLines>
                        <recordDelimiter>#x0A</recordDelimiter>
