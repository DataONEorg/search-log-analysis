                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Common Loon Behavioral, PAH and Health Data, 2015, Louisiana</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>COLO, Louisiana 2015 Data</gco:CharacterString>
