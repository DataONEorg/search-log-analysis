    <gmd:citation>
      <gmd:CI_Citation>
        <gmd:title>
          <gco:CharacterString>CTD Hydrographic data from Cruise OC468, LA, MS, AL Self Break, August and September, 2010</gco:CharacterString>
        </gmd:title>
        <gmd:alternateTitle>
          <gco:CharacterString/>
        </gmd:alternateTitle>
        <gmd:date>
            <gmd:CI_Date>
