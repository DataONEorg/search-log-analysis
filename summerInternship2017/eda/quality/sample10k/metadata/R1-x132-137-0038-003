            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Remote Sensing Data for Anthropogenic Chronic Oil Discharge - Offshore Louisiana, 2004-2012</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
