        <gmd:MD_DataIdentification>
            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>1999 Landsat Thematic Mapper 5 Climate Data Record Path 22 and Rows 39 and 40 surface reflectance and Normalized Difference Vegetation Index (NDVI) data of southeastern Louisiana marshes for phenology study</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString>1999 Landsat TM LEDAPS surface reflectance and NDVI marsh data sets</gco:CharacterString>
                    </gmd:alternateTitle>
                    <gmd:date>
                        <gmd:CI_Date>
                            <gmd:date>
