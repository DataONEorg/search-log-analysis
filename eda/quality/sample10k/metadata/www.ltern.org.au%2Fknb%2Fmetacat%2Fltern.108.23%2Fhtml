            </physical>
            <attributeList id="6b8b58f0-aa16-4ba5-a5b6-9054d4edb37a">
                <attribute id="1429258704002">
                    <attributeName>year</attributeName>
                    <attributeDefinition>The year of the survey</attributeDefinition>
                    <measurementScale>
                        <dateTime>
                            <formatString>YYYY</formatString>
                        </dateTime>
                    </measurementScale>
                </attribute>
