    <project>
      <title>Georgia Coastal Ecosystems LTER Project</title>
      <personnel>
        <individualName>
          <givenName>James</givenName>
          <givenName>T.</givenName>
          <surName>Hollibaugh</surName>
        </individualName>
        <role>principalInvestigator</role>
      </personnel>
