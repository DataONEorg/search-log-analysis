        <attribute>
          <attributeName>sensClassZ</attributeName>
          <attributeDefinition>Sensitivity class based on comparison of 2012 and 2013 normalized yield, as described in Zipper et al. (in review)</attributeDefinition>
          <storageType>string</storageType>
          <measurementScale>
            <nominal>
              <nonNumericDomain>
                <enumeratedDomain>
                  <codeDefinition>
                    <code>Consistenty Poor </code>
                    <definition> sample greater than 1 standard deviation from the origin and z-score below 0 in both years when 2012 and 2013 yield z-scores plotted in Cartesian coordinates</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>Consistently Medium </code>
                    <definition> sample within 1 standard deviation of the origin when 2012 and 2013 yield z-scores plotted in Cartesian coordinates</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>Consistently Strong </code>
                    <definition> sample greater than 1 standard deviation from the origin and z-score above 0 in both years when 2012 and 2013 yield z-scores plotted in Cartesian coordinates</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>Drought Sensitive </code>
                    <definition> sample greater than 1 standard deviation from the origin and z-score below 0 in 2012 (dry year) and above 0 in 2013 (wet year) when 2012 and 2013 yield z-scores plotted in Cartesian coordinates</definition>
                  </codeDefinition>
                  <codeDefinition>
                    <code>Wet Sensitive </code>
                    <definition> sample greater than 1 standard deviation from the origin and z-score above 0 in 2012 (dry year) and below 0 in 2013 (wet year) when 2012 and 2013 yield z-scores plotted in Cartesian coordinates</definition>
                  </codeDefinition>
                </enumeratedDomain>
              </nonNumericDomain>
            </nominal>
          </measurementScale>
        </attribute>
