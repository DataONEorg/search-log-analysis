        <title>Habitat--Offshore of Half Moon Bay, California</title>
        <geoform>polygon shapefile digital data</geoform>
        <lworkcit>
          <citeinfo>
            <origin>Nadine E. Golden</origin>
            <pubdate>2013</pubdate>
            <title>California State Waters Map Series Data Catalog</title>
            <geoform>database</geoform>
