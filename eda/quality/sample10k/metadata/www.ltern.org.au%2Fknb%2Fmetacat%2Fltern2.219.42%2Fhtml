            <individualName>
                <salutation>Dr</salutation>
                <givenName>Henrik</givenName>
                <surName>Wahren</surName>
            </individualName>
            <organizationName>La Trobe University</organizationName>
            <positionName>Plot network contacts</positionName>
            <address>
                <deliveryPoint>Research Centre for Applied Alpine Ecology</deliveryPoint>
                <deliveryPoint>Department of Agricultural Sciences, La Trobe University,</deliveryPoint>
                <city>Melbourne</city>
                <administrativeArea>Victoria 3</administrativeArea>
                <postalCode>3086</postalCode>
            </address>
