        <personnel>
          <individualName>
            <givenName>Laura</givenName>
            <surName>Ogden</surName>
          </individualName>
          <organizationName>Florida Coastal Everglades LTER Program</organizationName>
          <address>
            <deliveryPoint>Department of Global &amp; Sociocultural Studies</deliveryPoint>
            <deliveryPoint> Florida International University</deliveryPoint>
            <deliveryPoint> University Park</deliveryPoint>
            <deliveryPoint> DM341C</deliveryPoint>
