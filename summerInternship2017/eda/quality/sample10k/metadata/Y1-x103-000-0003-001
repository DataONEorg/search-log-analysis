    <gmd:identificationInfo>
        <gmd:MD_DataIdentification>
            <gmd:citation>
                <gmd:CI_Citation>
                    <gmd:title>
                        <gco:CharacterString>Northern Gulf of Mexico Salt Marsh Climate Data: Temperature, Pressure, and Wind Characteristics 2009-2010.</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString/>
                    </gmd:alternateTitle>
                    <gmd:date>
                        <gmd:CI_Date>
