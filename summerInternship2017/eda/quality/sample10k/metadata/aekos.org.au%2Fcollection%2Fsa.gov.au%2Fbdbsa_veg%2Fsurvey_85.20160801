    <dataset>
        <title>Deep Swamp Survey, Biological Survey of South Australia - Vegetation Survey, Biological Database of South Australia</title>
        <creator id="Department of Environment, Water and Natural Resources">
            <organizationName>Department of Environment, Water and Natural Resources</organizationName>
            <phone>+61 8 8204 1910</phone>
            <electronicMailAddress>DEWNRBioDataRequests&amp;#64;sa.gov.au</electronicMailAddress>
