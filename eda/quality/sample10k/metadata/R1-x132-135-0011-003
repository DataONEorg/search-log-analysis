    <gmd:citation>
      <gmd:CI_Citation>
        <gmd:title>
          <gco:CharacterString>MIMOSA sediment chamber 16S rRNA gene sequence data, Gulf of Mexico, October 1, 2013 to March 30, 2014</gco:CharacterString>
        </gmd:title>
        <gmd:alternateTitle>
          <gco:CharacterString/>
        </gmd:alternateTitle>
